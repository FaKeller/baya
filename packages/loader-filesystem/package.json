{
  "name": "@baya/loader-filesystem",
  "version": "0.0.1",
  "description": "File-system loader for baya",
  "keywords": [
    "baya"
  ],
  "directories": {
    "test": "jest"
  },
  "scripts": {
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "build": "npm run ts:build",
    "ts:build": "tsc",
    "ts:watch": "tsc -w",
    "ts:lint": "tslint -c tslint.json -p tsconfig.json",
    "depcruise": "depcruise --validate .dependency-cruiser.json src"
  },
  "author": "Fabian Keller <contact@fabian-keller.de>",
  "license": "MIT",
  "dependencies": {
    "@baya/core": "^0.0.1",
    "glob": "7.1.4",
    "js-yaml": "3.13.1",
    "md5": "2.2.1",
    "rxjs": "6.5.2",
    "transliteration": "2.1.5"
  },
  "devDependencies": {
    "@types/glob": "7.1.1",
    "@types/js-yaml": "3.12.1",
    "@types/md5": "2.1.33"
  }
}
