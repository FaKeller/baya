{
  "name": "@baya/core",
  "version": "0.0.1",
  "description": "Static site generator framework featuring React",
  "directories": {
    "test": "jest"
  },
  "scripts": {
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "build": "npm run ts:build",
    "ts:build": "tsc",
    "ts:watch": "tsc -w",
    "ts:lint": "tslint -c tslint.json -p tsconfig.json",
    "depcruise": "depcruise --validate .dependency-cruiser.json src"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:mimacom/website/assemblyline.git"
  },
  "author": "Fabian Keller <fabian.keller@mimacom.com>",
  "license": "",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFiles": [
      "./src/jest-bootstrap.ts"
    ]
  },
  "devDependencies": {
    "@types/i18next": "12.1.0",
    "@types/jest": "24.0.17",
    "@types/lodash": "4.14.136",
    "@types/markdown-it": "0.0.8",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.8.5",
    "@types/strftime": "0.9.2",
    "@types/winston": "2.4.4",
    "jest": "24.8.0",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0"
  },
  "dependencies": {
    "i18next": "17.0.10",
    "inversify": "5.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-inject-decorators": "3.1.0",
    "lodash": "4.17.15",
    "markdown-it": "9.0.1",
    "markdown-it-for-inline": "0.1.1",
    "md5": "2.2.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2",
    "strftime": "0.10.0",
    "winston": "3.2.1"
  }
}
